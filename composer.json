{
    "require": {
        "php": ">=5.5.9",
        "laravel/lumen-framework": "5.2.*",
        "vlucas/phpdotenv": "~1.0",
        "pusher/pusher-php-server": "^2.2",
	  	"laravel/cashier": "~6.0",
        "doctrine/dbal": "^2.5",
        "rdehnhardt/lumen-maintenance-mode": "^1.1",
        "drewm/mailchimp-api": "^2.1",
        "tymon/jwt-auth": "^1.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "fzaninotto/faker": "~1.0",
	  	"barryvdh/laravel-ide-helper": "^2.1",
        "rap2hpoutre/laravel-log-viewer": "^0.5.2",
        "mockery/mockery": "^0.9.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/"
        ],
		"files": [
		  "app/helpers.php"
		]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
